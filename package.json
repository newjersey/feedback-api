{
  "name": "feedback-api",
  "version": "1.0.0",
  "description": "aws-nodejs-typescript template",
  "main": "infra/feedback.ts",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:api": "cd infra && npx cdk deploy FeedbackApiStack",
    "deploy:db:dev": "cd infra && cdk deploy FeedbackDbStack --profile innov-platform-dev --app 'npx tsx bin/feedback.ts'",
    "diff": "cdk diff",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "synth:api": "cd infra && npx cdk synth FeedbackApiStack",
    "synth:db:dev": "cd infra && cdk synth --profile innov-platform-dev --app 'npx tsx bin/feedback.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "watch": "tsc -w",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prisma-migrate:reset": "npx prisma migrate reset",
    "prisma-migrate:dev": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@aws-sdk/client-comprehend": "3.879.0",
    "@aws-sdk/client-ssm": "3.879.0",
    "@azure/openai": "1.0.0-beta.13",
    "@middy/core": "3.6.2",
    "@middy/http-json-body-parser": "3.6.2",
    "aws-cdk-lib": "2.213.0",
    "aws-sdk": "2.1692.0",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-jest": "4.1.0",
    "constructs": "^10.0.0",
    "dotenv": "^17.2.1",
    "googleapis": "109.0.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.152",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.110",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "aws-cdk": "2.1027.0",
    "esbuild": "0.25.9",
    "eslint": "9.35.0",
    "eslint-config-prettier": "8.10.2",
    "eslint-plugin-prettier": "4.2.5",
    "jest": "29.7.0",
    "json-schema-to-ts": "1.6.5",
    "prettier": "2.8.8",
    "prisma": "6.15.0",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "3.15.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "license": "MIT"
}
